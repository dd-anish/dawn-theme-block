<div class="theme-blocks">
    {% content_for 'blocks' %}

    <p class="text"></p>
        {{ block.settings.text }}
    </p>
</div>

{% comment %} 

@app : App Block
@theme : Theme Block
 
{% endcomment %}

{% schema %}
{
  "name": "Text",
  "blocks": [{"type": "@app"}, {"type": "@theme"}],
  "settings": [
    {
      "type": "text",
      "id": "text",
      "label": "Text",
      "default": "Some Example Text"
    }
  ],
  "presets":[{
      "name": "Test"
    }]
}
{% endschema %}
